{% extends 'FiThnitek/baseFrontFiThnitek.html.twig' %}
{% block event %}
{% endblock %}
{% block container %}
    <div class="widget dashboard-container my-adslist">
    <h3 class="widget-header">Mes Ratings</h3>
    <table class="table table-responsive product-dashboard-table">
        <thead>
        <tr>
            <th>Identifiant</th>
            <th>nom</th>
            <th class="text-center">prenom</th>
            <th class="text-center">voir</th>

        </tr>
        </thead>
        <tbody>
    {% for c in User %}
        <tr>

            <td class="product-thumb"> <h2>{{ c.id }}</h2></td>
            <td class="product-details">
                <h2>{{ c.username }}</h2>
            </td>
            <td class="product-category"><center>{{ c.prenom }}</center></td>

            <td class="action" data-title="Action">
                <div class="">
                    <ul class="list-inline justify-content-center">
                        <li class="list-inline-item">
                            <a data-toggle="tooltip" data-placement="top" title="view" class="view" href="{{ path('fi_thnitek_addrating' , { 'id':c.id} ) }}">
                                <i class="fa fa-eye"></i>
                            </a>
                        </li>

                    </ul>
                </div>
            </td>

        </tr>
    {% endfor %}
        </tbody>
    </table>

    </div>
{% endblock %}